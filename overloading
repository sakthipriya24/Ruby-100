class Test 
    def self.sum(a,b) 
        puts(a+b) 
    end
    def self.sum(a,b,c) 
        puts(a+b+c) 
    end
      
end
Test.sum(1,2) #error

class Test 
    def self.sum(a,b) 
        puts(a+b) 
    end
    def self.sum(a,b,c) 
        puts(a+b+c) 
    end
      
end
Test.sum(1,2,7) #prints 10
